    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@argo/crypto/(.*)": "<rootDir>/libs/crypto/src/$1",
      "@argo/crypto": "<rootDir>/libs/crypto/src",
      "@adopt/redis/(.*)": "<rootDir>/libs/redis/src/$1",
      "@adopt/redis": "<rootDir>/libs/redis/src",
      "@argo/redis/(.*)": "<rootDir>/libs/redis/src/$1",
      "@argo/redis": "<rootDir>/libs/redis/src",
      "@argo/jwk/(.*)": "<rootDir>/libs/jwk/src/$1",
      "@argo/jwk": "<rootDir>/libs/jwk/src"
    }
  }
